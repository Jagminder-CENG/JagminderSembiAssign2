            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/Rent3"
                android:textStyle="bold"
                tools:layout_editor_absoluteX="144dp"
                tools:layout_editor_absoluteY="608dp"
                android:layout_gravity="center"/>


            <CheckBox
                android:id="@+id/checkBox3"
                android:layout_width="143dp"
                android:layout_height="28dp"
                android:checked="true"
                android:text="@string/Visit2"
                android:layout_gravity="center"/>
            <ImageView
                android:layout_width="220dp"
                android:layout_height="179dp"
                android:background="@drawable/apt4"
                app:layout_constraintHeight_percent=".12"
                app:layout_constraintWidth_percent=".20"
                tools:layout_editor_absoluteX="8dp"
                tools:layout_editor_absoluteY="518dp"
                android:layout_gravity="center"/>

            <TextView
                android:id="@+id/JagminderAddress4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/Address4"
