            android:orientation="vertical">


            <TextView
                android:id="@+id/JagminderApt"
                android:layout_width="220dp"
                android:layout_height="54dp"
                android:layout_gravity="center"
                android:backgroundTint="#E8ACAC"
                android:text="@string/Apt"
                android:textAppearance="@style/TextAppearance.AppCompat.Body1"
                android:textSize="45dp"
                android:textStyle="bold" />

            <ImageView
                android:layout_width="220dp"
                android:layout_height="179dp"
                android:background="@drawable/apt1"
                app:layout_constraintHeight_percent=".12"
                app:layout_constraintWidth_percent=".20"
                tools:layout_editor_absoluteX="4dp"
                tools:layout_editor_absoluteY="7dp"
                android:layout_gravity="center"/>



            <TextView
                android:id="@+id/JagminderAddress1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/Address"
                android:textStyle="bold"
                tools:layout_editor_absoluteX="141dp"
                tools:layout_editor_absoluteY="120dp"
                android:layout_gravity="center"
                android:textAppearance="@style/TextAppearance.AppCompat.Body1"
                />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/Rent"
                android:textStyle="bold"
                tools:layout_editor_absoluteX="141dp"
                tools:layout_editor_absoluteY="82dp"
                android:layout_gravity="center"/>



            <CheckBox
                android:id="@+id/checkBox1"
                android:layout_width="143dp"
                android:layout_height="28dp"
                android:checked="true"
                android:text="@string/Visit2"
                android:layout_gravity="center"/>

            <ImageView
                android:layout_width="220dp"
                android:layout_height="179dp"
                android:background="@drawable/apt2"
                app:layout_constraintHeight_percent=".12"
                app:layout_constraintWidth_percent=".20"
                tools:layout_editor_absoluteX="10dp"
                tools:layout_editor_absoluteY="345dp"
                android:layout_gravity="center"/>

            <TextView
                android:id="@+id/JagminderAddress2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/Address2"
